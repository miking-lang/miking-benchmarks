class BirthDeathParameter {
  λ:Random<Real>;
  μ:Random<Real>;

  override function write(buffer:Buffer) {
    if λ.hasDistribution() {
      buffer.set("λ", λ.getDistribution());
    } else {
      buffer.set("λ", λ);
    }
    if μ.hasDistribution() {
      buffer.set("μ", μ.getDistribution());
    } else {
      buffer.set("μ", μ);
    }
  }
}
