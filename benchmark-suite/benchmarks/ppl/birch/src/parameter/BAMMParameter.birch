class BAMMParameter < BirthDeathParameter {
  η:Random<Real>;
  z:Random<Real>;
  ν:Integer;
  ξ:Real;

  override function write(buffer:Buffer) {
    super.write(buffer);
    if η.hasDistribution() {
      buffer.set("η", η.getDistribution());
    } else {
      buffer.set("η", η);
    }
    buffer.set("z", z);
    buffer.set("ν", ν);
    buffer.set("ξ", ξ);
  }
}
